			</FIELDS>
			<KEYS>
				<KEY NAME="primary" TYPE="primary" FIELDS="id" />
			</KEYS>
		</TABLE>
			
		<TABLE NAME="block_webgd_pagina_order" COMMENT=""> 
			<FIELDS>
